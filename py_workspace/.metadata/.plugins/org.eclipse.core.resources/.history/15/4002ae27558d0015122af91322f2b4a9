	setTimeout(remove1, 14000);
	setTimeout(remove2, 15000);
	setTimeout(remove3, 16000);
	setTimeout(remove4, 17000);
	setTimeout(remove5, 18000);
	setTimeout(remove6, 19000);
	setTimeout(remove7, 20000);
	
